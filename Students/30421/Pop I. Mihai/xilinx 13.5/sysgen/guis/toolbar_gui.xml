<sysgenblock simulinkname="Toolbar" block_type="toolbar">
  <icon width="50" height="54" wmark_color="blue" bg_color="white"/>
  <sfunction name="xlpermissive"/>
  <callbacks OpenFcn="xltoolbar;"/>
  <libraries>
    <library name="xbsIndex"/>
    <library name="xbsTools"/>
  </libraries>
  <blockgui label="Xilinx Toolbar">
    <editbox name="infoedit" default="Toolbar" read_only="true" multi_line="true" evaluate="false"/>
    <editbox name="func" label="Launch function" default="xltoolbar" evaluate="false"/>
    <hiddenvar name="ShowPortLabels"/>
  </blockgui>
</sysgenblock>
