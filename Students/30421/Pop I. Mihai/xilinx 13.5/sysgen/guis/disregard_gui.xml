<sysgenblock simulinkname="Disregard Subsystem" tag="discardX" block_type="disregard">
  <initialization file="xldisregard_init.m"/>
  <icon width="58" height="58" wmark_color="darkgray" bg_color="black" caption_format="Disregard Subsystem\nFor Generation"/>
  <sfunction name="xlpermissive"/>
  <libraries>
    <library name="xbsIndex"/>
    <library name="xbsTools"/>
  </libraries>
  <blockgui empty="true" label="Xilinx Disregard Subsystem For Generation">
    <editbox name="infoedit" default="Place this block into a subsystem to have System Generator ignore the subsystem during code generation. This block can be used in combination with the Simulation Multiplexer block to provide an alternative simulation model for another subsystem (e.g., to provide a simulation model for a black box)." read_only="true" evaluate="false" multi_line="true"/>

    <hiddenvar name="ShowPortLabels"/>
  </blockgui>
</sysgenblock>
