<sysgenblock simulinkname="Copyright" block_type="copyright">
  <initialization file="xlcopyright_init.m"/>
  <icon width="200" height="100" wmark_color="beige" bg_color="white" file="copyright_icon.m"/>
  <sfunction name="xlpermissive"/>
  <doc doccommand="disp(sprintf('\n   There is no help documentation for the Xilinx Copyright Notice block.\n\n'))"/>
  <libraries>
    <library name="xbsCopyrightNotice"/>
  </libraries>
  <blockgui label="Xilinx Copyright Notice">
    <editbox name="copyrighttext" read_only="true" multi_line="true" evaluate="false"/>
    <hiddenvar name="infoedit"/>    
    <hiddenvar name="ShowPortLabels"/>    
  </blockgui>
</sysgenblock>
