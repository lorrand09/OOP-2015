<sysgenblock simulinkname="Configurable Subsystem Manager" tag="xcfgMgr" block_type="configurable_subsystem_manager">
  <icon width="53" height="51" wmark_color="blue" bg_color="white" caption_format="Configurable Subsystem\nManager"/>
  <sfunction name="xlpermissive"/>
  <callbacks DeleteFcn="xlconfig_mgr_callback(gcb, 'DeleteFcn');" OpenFcn="xlconfig_mgr_callback(gcb, 'OpenFcn');"/>
  <libraries>
    <library name="xbsIndex"/>
    <library name="xbsTools"/>
